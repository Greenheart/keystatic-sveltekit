{
  "name": "keystatic-svelte",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Samuel Plumppu",
    "url": "https://samuelplumppu.se"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.9.2",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "patchedDependencies": {
      "@keystatic/core": "patches/@keystatic__core.patch"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "dependencies": {
    "@keystatic/core": "^0.5.48",
    "esbuild": "^0.25.11",
    "markdoc-svelte": "^3.0.0",
    "zod": "^4.1.12"
  }
}
