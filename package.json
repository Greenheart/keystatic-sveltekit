{
  "name": "keystatic-sveltekit",
  "description": "Keystatic CMS integration for SvelteKit",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Samuel Plumppu",
    "url": "https://samuelplumppu.se"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Greenheart/keystatic-sveltekit"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "dependencies": {
    "esbuild": "^0.27.3",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.50.2",
    "@keystatic/core": "^0.5.48",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "vite": "^7.0.0"
  },
  "devDependencies": {
    "@keystatic/core": "^0.5.48",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/package": "^2.5.7",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/node": "^25.2.1",
    "markdoc-svelte": "^4.1.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "publint": "^0.3.17",
    "svelte": "^5.49.2",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "patchedDependencies": {
      "@keystatic/core": "patches/@keystatic__core.patch"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  }
}
